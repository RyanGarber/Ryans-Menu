{
  "name": "ryans-menu-installer",
  "version": "1.0.0",
  "description": "Installer for Ryan's Menu, a Lua script for Stand.",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanGarber/Ryans-Menu.git"
  },
  "author": "Ryan Garber",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/RyanGarber/Ryans-Menu/issues"
  },
  "homepage": "https://github.com/RyanGarber/Ryans-Menu#readme",
  "dependencies": {
    "https": "^1.0.0",
    "nw-builder": "^4.2.3-beta.2"
  },
  "devDependencies": {
    "nwjs-builder-phoenix": "^1.15.0"
  },
  "scripts": {
    "start": "run --x86 --mirror https://dl.nwjs.io/ .",
    "dist": "build --tasks win-x86 --mirror https://dl.nwjs.io/ ."
